{
	"name": "@oh-my-pi/omp-stats",
	"version": "6.9.69",
	"description": "Local observability dashboard for pi AI usage statistics",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"bin": {
		"omp-stats": "./src/index.ts"
	},
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./src/server": {
			"types": "./src/server.ts",
			"import": "./src/server.ts"
		},
		"./src/db": {
			"types": "./src/db.ts",
			"import": "./src/db.ts"
		}
	},
	"files": [
		"src",
		"public",
		"README.md"
	],
	"keywords": [
		"ai",
		"observability",
		"metrics",
		"dashboard",
		"llm",
		"statistics"
	],
	"scripts": {
		"dev": "bun run src/index.ts",
		"check": "tsgo --noEmit && tsgo --noEmit -p tsconfig.client.json",
		"build": "bun run build.ts && tsgo -p tsconfig.build.json"
	},
	"author": "Can Bölük",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/can1357/oh-my-pi.git",
		"directory": "packages/omp-stats"
	},
	"dependencies": {
		"@oh-my-pi/pi-ai": "workspace:*",
		"date-fns": "^4.1.0",
		"lucide-react": "^0.562.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"recharts": "^3.6.0"
	},
	"devDependencies": {
		"@types/node": "^24.3.0",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3"
	},
	"engines": {
		"bun": ">=1.0.0"
	}
}
